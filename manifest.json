{
  "manifest_version": 2,
  "homepage_url": "https://github.com/schovi/webpack-chrome-extension",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
	  "js": ["web3/index.js", "web3/inpage.js"],
	  "run_at": "document_start"
	},
    {
      "matches": ["<all_urls>"],
	  "js": ["content/index.js"]
	}
  ],
  "web_accessible_resources": [
	  "web3/inpage.js",
	  "web3/embed.html",
	  "./web3/embed.js",
	  "./web3/embed.js.map",
	  "./web3/commons.js",
	  "./web3/commons.js.map",
	  "./web3/vendor.js"
  ],
  "background": {
    "scripts": ["background/index.js"]
  },
  "__content_security_policy": "script-src http://localhost:3000;style-src 'self' 'unsafe-inline';default-src 'self'",
  "permissions": [
	  "storage",
	  "tabs"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "128": "icons/icon-128.png"
  }
}
